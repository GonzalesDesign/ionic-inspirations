{
   "extends": "../tslint.json",
   "rules": {
     "directive-selector": [
       true,
       "attribute",
       "app",
       "camelCase"
     ],
     "component-selector": [
       true,
       "element",
       "app",
       "page",
       "kebab-case"
     ],
     "max-line-length": [
         false,
         140
       ],
     "typedef": [
         false,
         "arrow-call-signature",
         "call-signature"
     ],
     "no-inferrable-types": false,
     "interface-name": false,
   }
 }
